          - _requestoropen: '(:? ()' 
          - date:             '(\d{2}/\d{2}/\d{4}) \s+'
          - time:             '(\d{2}:\d{2}:\d{2});'
          - flag:             '([SEQD]);'
          - jobid:            '([^;]+);'
