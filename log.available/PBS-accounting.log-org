          - listed_nodes:     'Resource_List[.]nodes=     (\S+) \s+' 
          - _memopen: '(:? ()'
          - listed_pmem:      'Resource_List[.]pmem=      (\d+\w+) \s+'
          - listed_pvmem:     'Resource_List[.]pvmem=     (\d+\w+) \s+'
          - _memclose: ')? ()'
          - listed_walltime:  'Resource_List[.]walltime=  (\d{2}:\d{2}:\d{2}) \s+'
          - _jobendopen: '(:? ()'
          - session:          'session=                   (\d+) \s+'
          - end:              'end=                       (\d+) \s+'
          - exit_status:      'Exit_status=               (\d+) \s+'
